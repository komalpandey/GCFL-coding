#include <stdio.h>
#include <string.h>
int main()
{
    int i,j,n;
    char str[20];
    gets(str);
    for(i=0;i<strlen(str);i++)
    {
        for(j=0;j<=i;j++)
        {
            printf("%c",str[j]);
        }
        
        printf("\n");
    }
    for(i=strlen(str);i>=0;i--)
    {
        for(j=0;j<=i;j++)
        {
            printf("%c",str[j]);
        }
        printf("\n");
    }

    return 0;
}
